{
  "name": "tricks",
  "version": "1.6.0",
  "description": "ES6 modules",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./",
    "test": "npm run lint && npm run test:node && npm run test:browser",
    "test:browser": "gulp",
    "test:node": "mocha ./test/specs/{time,object,string,http}/*.js",
    "test:spawn": "node_modules/.bin/browserstack-runner",
    "test:deploy": "npm test && npm run test:spawn",
    "test:spawn-local": "env $(cat .env | xargs) npm run test:spawn"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MrSwitch/tricks.git"
  },
  "keywords": [
    "Es6",
    "modules"
  ],
  "author": "Andrew Dodson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MrSwitch/tricks/issues"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "browser": {
    "./http/json.js": "./browser/http/json.js",
    "./http/request.js": "./browser/http/request.js",
    "./http/sendBeacon.js": "./browser/http/sendBeacon.js",
    "./test/stubs/http.js": "./test/stubs/xhr.js"
  },
  "homepage": "https://github.com/MrSwitch/tricks",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2016": "^6.11.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.0",
    "browserstack-runner": "^0.4.4",
    "eslint": "^3.10.2",
    "expect.js": "^0.3.1",
    "gulp": "^3.9.0",
    "gulp-mocha-phantomjs": "^0.12.1",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-util": "^3.0.6",
    "localhost": "^0.1.2",
    "mocha": "^2.5.3",
    "shunt": "^0.1.6",
    "sinon": "^1.17.4",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
}
